<% include ../layout/head %>
<% include ../layout/header %>

<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h1>All Articles</h1>
      <% articles.forEach(function(article){ %>
        <div class="well">
          <a href="/articles/<%= article.slug %>"><h1><%= article.title %></h1></a>
          <p><strong>By: <%= article.author %></strong></p>
        </div>
      <% }); %>
    </div>
  </div>
</div>

<% include ../layout/footer %>
<% include ../layout/foot %>